## WARNING Not bad performance for all
## if the SAME TRIPLE COUNT APPEARS it may be a loading issue

SELECT  ?triplesize (count(?triplesize) as ?triplecount)
WHERE     {
    SELECT  ?g (count(?s) as ?triplesize)
    WHERE     {
         GRAPH ?g {?s ?p ?o .}

    }
    GROUP BY ?g
         }
GROUP BY ?triplesize
ORDER BY DESC(?triplecount)